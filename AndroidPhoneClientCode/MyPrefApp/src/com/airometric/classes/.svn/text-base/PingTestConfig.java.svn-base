package com.airometric.classes;

import java.io.Serializable;

public class PingTestConfig implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -5664986537590829266L;
	public String sServerIP, sCycles;

	public PingTestConfig(String sServerIP, String sCycles) {
		this.sServerIP = sServerIP;
		this.sCycles = sCycles;
	}

	@Override
	public String toString() {
		String toString = "PingTestConfig() :: Server IP = " + sServerIP
				+ ", Cycles = " + sCycles;
		return toString;
	}
}
